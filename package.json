{
  "name": "create-oakwood-app",
  "private": true,
  "author": "Oakwood Creative",
  "version": "0.0.0",
  "description": "Oakwood App Scaffolding",
  "repository": {
    "type": "git",
    "url": "https://github.com/oakwood/create-oakwood-app.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oakwood/create-oakwood-app/issues"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "dev": "next",
    "start": "next start -p $PORT",
    "now-build": "build-storybook -c .storybook -s ./public -o .styleguide",
    "build:storybook": "build-storybook -c .storybook -s ./public -o .styleguide",
    "storybook": "start-storybook -s ./public -p 3001 --ci",
    "lint": "eslint . --cache --report-unused-disable-directives",
    "lint:ci": "eslint . --report-unused-disable-directives",
    "lint:fix": "eslint . --cache --fix",
    "prettier": "prettier --check \"src/**/*.js\"",
    "prettier:fix": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@oakwood/oui": "0.8.1",
    "clsx": "^1.1.0",
    "next": "^9.5.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "recompose": "^0.30.0",
    "swiper": "^5.3.6"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.5.3",
    "@storybook/addon-a11y": "^5.3.14",
    "@storybook/addon-actions": "^5.3.14",
